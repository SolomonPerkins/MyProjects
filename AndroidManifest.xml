<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myprofile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.myprofile.MainActivity"
            android:label="@string/home_link" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity 
            android:name="com.example.myprofile.AboutActivity"
            android:label="@string/title_activity_about"
            android:parentActivityName="com.example.myprofile.MainActivity">
  				
  				<meta-data 
  				    android:name="android.support.PARENT_ACTIVITY"
  				    android:value="com.example.myprofile.MainActivity"/>
  				
 	 	</activity>          
    </application>

</manifest>
